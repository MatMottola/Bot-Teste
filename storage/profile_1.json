  "UeSpell": "Exevo gran mas Frigo",
  "ingame_macros": "local spell = \"uteta res dru\"\nlocal cooldownTime = 60 -- tempo oficial do cooldown em segundos\nlocal lastCast = 0\n\n-- cria label na tela\nlocal cooldownLabel = UI.Label(\"Avatar: Ready\")\ncooldownLabel:setColor(\"green\")\n\nmacro(200, \"Auto Avatar\", function()\n  local now = now / 1000\n\n  -- Se pode castar, usa a spell\n  if canCast(spell) then\n    saySpell(spell)\n    lastCast = now\n  end\n\n  -- Calcula tempo restante\n  local remaining = math.max(0, cooldownTime - (now - lastCast))\n\n  if remaining > 0 then\n    cooldownLabel:setText(\"Avatar: \" .. math.ceil(remaining) .. \"s\")\n    cooldownLabel:setColor(\"blue\")\n  else\n    cooldownLabel:setText(\"Avatar: Ready\")\n    cooldownLabel:setColor(\"green\")\n  end\nend)\n\n-- Auto SSA Looter\n\n-- ===== CONFIG =====\nlocal SSA_ID = 3081\nlocal MIN_SSA = 5\nlocal MAX_SSA = 19\n-- ==================\n\nif not storage.ssaAutoLoot then\n    storage.ssaAutoLoot = {\n        mode = \"idle\" -- idle | adding | cleared\n    }\nend\n\n-- Fun��o para contar SSA (n�o stack�vel)\nlocal function getSSAcount()\n    local count = 0\n    \n    for _, container in pairs(g_game.getContainers()) do\n        for _, item in pairs(container:getItems()) do\n            if item:getId() == SSA_ID then\n                count = count + 1\n            end\n        end\n    end\n    \n    return count\nend\n\nmacro(2000, \"SSA AutoLoot Control\", function()\n    local total = getSSAcount()\n\n    -- ?? Se tiver menos que 5 SSA\n    if total < MIN_SSA and storage.ssaAutoLoot.mode ~= \"adding\" then\n        say(\"!autoloot add, stone skin amulet\")\n        storage.ssaAutoLoot.mode = \"adding\"\n        warn(\"SSA abaixo de 5 - Autoloot ADD ativado\")\n    end\n\n    -- ?? Se tiver 19 ou mais SSA\n    if total >= MAX_SSA and storage.ssaAutoLoot.mode ~= \"cleared\" then\n        say(\"!autoloot clear\")\n        storage.ssaAutoLoot.mode = \"cleared\"\n        info(\"SSA >= 19 - Autoloot CLEAR executado\")\n    end\n\n    -- ?? Reset interno quando ficar entre 5 e 18\n    if total >= MIN_SSA and total < MAX_SSA then\n        storage.ssaAutoLoot.mode = \"idle\"\n    end\nend)\n\n\n\n--\n\n\nlocal spell = \"exura sio\"\nlocal hpPercent = 80\nlocal minMana = 100\nlocal delay = 200\n\nmacro(delay, \"Guild SIO (Green Emblem)\", function()\n\n    if mana() < minMana then return end\n\n    local lowestTarget = nil\n    local lowestHp = 101\n\n    for _, creature in ipairs(getSpectators()) do\n        if creature:isPlayer()\n        and not creature:isLocalPlayer()\n        and creature:getEmblem() == 1\n        and creature:getHealthPercent() > 0\n        and creature:getHealthPercent() < hpPercent then\n\n            if creature:getHealthPercent() <= lowestHp then\n                lowestHp = creature:getHealthPercent()\n                lowestTarget = creature\n            end\n        end\n    end\n\n    if lowestTarget then\n        say(spell .. ' \"' .. lowestTarget:getName())\n    end\n\nend)\n\n\n\n\n\n",
